package main

import (
	"fmt"
	"io/ioutil"
	"os"
	"strings"
)

func main() {
	"fmt"
	"io/ioutil"
	"os"
	"strings"
}

func main() {
	counts := make(map[string]int)
	for _, filename := range os.Args[1:] {
		data, err := ioutil.ReadFile(filename)
		if err != nil {
			fmt.Fprintf(os.Stderr, "dup3: %v\n", err)
			continue
		}
		for _, line := range strings.Split(string(data), "\n") {
			counts[line]++
		}
	}
	for line, n := range counts {
		if n > 1 {
			fmt.Printf("%d\t%s\n", n, line)
		}
	}
}

/*
ReadFile returns a byte slice that must be converted intoa string so it
can be split by strings.Split. We will discuss strings and bytes slices at length.



*/
